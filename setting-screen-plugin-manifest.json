            "type": "text_input"
          },
          {
            "key": "cleeng_settings_menu_item5",
            "label": "Item 5 Name",
            "label_tooltip": "Enter the name that should be displayed fpr item 5 in the settings menu",
            "type": "text_input"
          },
          {
            "key": "cleeng_settings_welcome_message",
