{
  "api": {
    "require_startup_execution": true,
    "class_name": "CleengLogin.ZappCleengSettingScreenManager"
  },
  "dependency_repository_url": [
    "git@github.com:applicaster/login-plugin-cleeng-ios.git"
  ],
  "platform": "ios",
  "author_name": "Anton Klysa",
  "author_email": "a.klysa@avantize.com",
  "manifest_version": "1.2.6",
  "name": "Cleeng Settings Screen Plugin",
  "description": "Cleeng setting screen plugin - to be used with the same version of the cleeng plugin",
  "type": "general",
  "screen": true,
  "identifier": "cleeng_settings_plugin",
  "ui_builder_support": true,
  "dependency_name": "CleengLogin",
  "dependency_version": "1.2.6",
  "whitelisted_account_ids": [
      "5c209d8de209ac000d3c4250",
      "5c7e272b51a78800083d1187",
      "5af949534da8f300080050f9",
      "572a0a65373163000b000000"
  ],
  "min_zapp_sdk": "10.5.0-Dev",
  "deprecated_since_zapp_sdk": "",
  "unsupported_since_zapp_sdk": "",
  "react_native": false,
  "custom_configuration_fields": [
    {
      "key": "ios_assets_bundle",
      "type": "uploader"
    },
    {
      "key": "android_assets_bundle",
      "type": "uploader"
    }
  ],
  "data": {
    "fields": [
      {
        "key": "source",
        "type": "text_input"
      },
      {
        "key": "type",
        "type": "select"
      }
    ]
  },
  "styles": {
    "fields": [
      {
        "key": "presentation",
        "type": "hidden",
        "initial_value": "push"
      },
      {
        "key": "cleeng_settings_user_name_font",
        "type": "ios_font_selector",
        "label": "User Name Font"
      },
      {
        "key": "cleeng_settings_user_name_font_android",
        "type": "android_font_selector",
        "label": "User Name Font Android"
      },
      {
        "key": "cleeng_settings_user_name_size",
        "type": "number_input",
        "label": "User Name Font Size"
      },
      {
        "key": "cleeng_settings_user_name_color",
        "type": "color_picker",
        "label": "User Name Font Color"
      },
      {
        "key": "cleeng_settings_view_profile_font",
        "type": "ios_font_selector",
        "label": "View Profile Font"
      },
      {
        "key": "cleeng_settings_view_profile_font_android",
        "type": "android_font_selector",
        "label": "View Profile Font Android"
      },
      {
        "key": "cleeng_settings_view_profile_size",
        "type": "number_input",
        "label": "View Profile Font Size"
      },
      {
        "key": "cleeng_settings_view_profile_color",
        "type": "color_picker",
        "label": "View Profile Font Color"
      },
      {
        "key": "cleeng_settings_menu_item_text_font",
        "type": "ios_font_selector",
        "label": "Settings Item Font"
      },
      {
        "key": "cleeng_settings_menu_item_text_font_android",
        "type": "android_font_selector",
        "label": "Settings Item Font Android"
      },
      {
        "key": "cleeng_settings_menu_item_text_size",
        "type": "number_input",
        "label": "Settings Item Font Size"
      },
      {
        "key": "cleeng_settings_menu_item_text_color",
        "type": "color_picker",
        "label": "Settings Item Font Color"
      },
      {
        "key": "menu_item_bg_color",
        "label": "Default color background",
        "type": "color_picker"
      },
      {
        "key": "menu_item_selected_bg_color",
        "label": "Selected color background",
        "type": "color_picker"
      },
      {
        "key": "cleeng_settings_screen_background_color",
        "label": "Settings main background color",
        "type": "color_picker"
      }
    ]
  },
  "general": {
    "fields": [
      {
        "key": "isSettings",
        "label": "Is this screen a settings screen",
        "label_tooltip": "Mark true for setting screen",
        "type": "switch",
        "initial_value": "true"
      },
      {
        "group": true,
        "label": "Setting screen Items",
        "folded": false,
        "fields": [
          {
            "key": "cleeng_settings_login_text",
            "label": "Login text",
            "label_tooltip": "",
            "default": "LOGIN",
            "type": "text_input"
          },
          {
            "key": "cleeng_settings_logout_text",
            "label": "Logout text",
            "label_tooltip": "",
            "default": "LOGOUT",
            "type": "text_input"
          },
          {
            "key": "cleeng_settings_view_profile_text",
            "label": "View Profile Text",
            "label_tooltip": "",
            "default": "View Profile",
            "type": "text_input"
          },
          {
            "key": "cleeng_settings_menu_url1",
            "label": "Item 1 Action or URL",
            "label_tooltip": "Enter the URL that should be opened by item 1 in the settings menu or the action",
            "type": "text_input"
          },
          {
            "key": "cleeng_settings_menu_image_url1",
            "label": "Item 1 image url keys",
            "label_tooltip": "Enter the image url key for item 1 as uploaded in the assets bundle of the plugin",
            "type": "text_input"
          },
          {
            "key": "cleeng_settings_menu_item1",
            "label": "Item 1 Name",
            "label_tooltip": "Enter the name that should be displayed fpr item 1 in the settings menu",
            "type": "text_input"
          },
          {
